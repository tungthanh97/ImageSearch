{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx", ""]
      }
    }
  },
  "rules": {
    "no-unused-vars": 1,
    "import/export": 0,
    "import/extensions": "off",
    "import/no-unresolved": [1, { "caseSensitive": false }]
  }
}
